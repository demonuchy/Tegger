{
    "version": 2,
    "builds": [
      {
        "src": "tgappfront/package.json", 
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      },
      {
        "src": "app/api/**/*.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/tgappfront/build/$1"
      }
    ]
  }